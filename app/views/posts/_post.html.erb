<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= post.description %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= post.image %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= post.location %>
  </p>

  <p>
    <strong>Time:</strong>
    <%= post.time %>
  </p>

</div>
